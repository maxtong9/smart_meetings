<%= form_with(model: meeting, local: true) do |form| %>

  <div class="row py-2" style="margin:0 0 0 40px;">
    <%= form.file_field :my_file, multiple: true, :required => true, :class => "btn" %>
  </div>

  </div>
  <table class="table w-auto table-borderless mb-0 span2">
    <tr class="row">
      <td style="width: 65%"><%= link_to 'Back', meetings_path, class:"btn btn-outline-secondary" %></td>
      <td style="width: 35%"> <%= form.submit "Upload Files", class:'btn btn-outline-secondary'%></td>
    </tr>
  </table>

<% end %>
